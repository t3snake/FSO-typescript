    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Patientor Frontend",
            "cwd":"${workspaceFolder}/patientor/frontend",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/patientor/frontend/src/App.tsx",
            "preLaunchTask": "npm run dev",
            "outFiles": [
                "${workspaceFolder}/patientor/frontend/dist/*.js"
            ]
        }
    ]
}